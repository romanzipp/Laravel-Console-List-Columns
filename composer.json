{
    "name": "romanzipp/laravel-console-list-columns",
    "description": "List database tables columns & information.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "doctrine/dbal": "^2.7|^3.0",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0",
        "spatie/emoji": "^2.1",
        "symfony/console": "^4.0|^5.0|^6.0",
        "wujunze/php-cli-color": "^2.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": ">=3.8",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^8.5.30|^9.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\ColumnList\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\ColumnList\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\ColumnList\\Providers\\ColumnListProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
