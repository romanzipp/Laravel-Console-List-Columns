{
    "name": "romanzipp/laravel-console-list-columns",
    "description": "List database tables columns & information.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "symfony/console": "^4.0|^5.0",
        "wujunze/php-cli-color": "^2.4",
        "spatie/emoji": "^2.1",
        "doctrine/dbal": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^8.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\ColumnList\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\ColumnList\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\ColumnList\\Providers\\ColumnListProvider"
            ]
        }
    }
}
